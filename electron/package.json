{
  "name": "Restvo",
  "version": "1.7.21",
  "description": "Connect with Mentors",
  "main": "index.js",
  "private": true,
  "scripts": {
    "electron:start": "electron ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw --x64 --ia32",
    "publish": "electron-builder -mw --x64 --ia32 -p always"
  },
  "build": {
    "appId": "com.restvo.electron",
    "productName": "Restvo",
    "buildVersion": "1",
    "copyright": "Copyright Â© 2020 Restvo Inc.",
    "mac": {
      "category": "Social Networking",
      "darkModeSupport": true
    },
    "win": {
      "target": "NSIS"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "calvinckho",
        "repo": "restvoApp"
      }
    ]
  },
  "dependencies": {
    "@capacitor/electron": "^2.2.1",
    "ajv": "^6.12.3",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^1.3.0",
    "electron-push-receiver": "^2.1.3",
    "electron-updater": "^4.3.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "electron": "8.4.0",
    "electron-builder": "^22.7.0"
  },
  "keywords": [
    "capacitor",
    "electron"
  ],
  "author": "Calvin Ho",
  "license": "ISC"
}
